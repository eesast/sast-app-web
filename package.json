{
  "name": "sast-app-web",
  "version": "1.1.0",
  "private": true,
  "dependencies": {
    "antd": "^3.12.1",
    "axios": "^0.18.0",
    "dompurify": "^1.0.8",
    "highlight.js": "^9.13.1",
    "jwt-decode": "^2.2.0",
    "marked": "^0.6.0",
    "moment": "^2.23.0",
    "react": "^16.7.0",
    "react-document-title": "^2.0.3",
    "react-dom": "^16.7.0",
    "react-infinite-scroller": "^1.2.4",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1"
  },
  "devDependencies": {
    "@craco/craco": "^3.2.3",
    "@types/jest": "^23.3.12",
    "craco-antd": "^1.9.3",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "react-scripts test",
    "eject": "craco eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!node_modules/",
      "!src/index.js",
      "!src/serviceWorker.js",
      "!src/setupTests.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "CI=true npm test --findRelatedTests && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
